<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]
        >

<hibernate-mapping>

	<class name="com.coreweb.domain.AgendaEventoDetalle"
		table="AgendaEventoDetalle"> 

		&hibernate-domain; 


		<property name="fecha" />
		<property name="texto" />
		<property name="tipo" />
		<property name="link" />
		<property name="usuario" />
				
	</class>
</hibernate-mapping>
